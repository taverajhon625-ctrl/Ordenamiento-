def bubble_sort(lista):
    n = len(lista)
    # Recorrer todos los elementos de la lista
    for i in range(n):
        # El último i elementos ya están en su lugar
        for j in range(0, n - i - 1):
            # Recorrer la lista de 0 a n-i-1
            # Intercambiar si el elemento encontrado es mayor que el siguiente
            if lista[j] > lista[j + 1]:
                lista[j], lista[j + 1] = lista[j + 1], lista[j]

# Tu lista de números
datos = [7, 1434, 2701, 4394, 3642, 4426, 8896, 8532, 4327, 8098, 8234, 9529, 2012, 7245, 4571, 75, 3334, 4773, 3311, 7984, 252, 8742, 6848, 6786, 5373, 4780, 268, 5648, 7663, 3648, 5502, 2547, 9164, 361, 7779, 4382, 2880, 9325, 8037, 4219, 2075, 9260, 8770, 7624, 8798, 4105, 3393, 2615, 8597, 4063, 5958, 9143, 1847, 9033, 2575, 9796, 3317, 8969, 4203, 4864, 2684, 4075, 4157, 1098, 3903, 3859, 4726, 2256, 5333, 2193, 2662, 5146, 1250, 108, 780, 2863, 414, 2615, 8871, 2640, 5022, 2766, 8305, 4828, 475, 5020, 6387, 1614, 2811, 388, 5231, 1454, 2940, 5323, 2068, 5990, 8461, 5178, 355, 8408, 2463, 8258, 967, 4390, 2689, 9752, 2753, 3399, 9903, 9541, 4888, 5505, 3478, 7275, 3926, 1921, 5310, 5391, 3412, 3056, 867, 5999, 2402, 1950, 263, 7674, 7951, 312, 7898, 2965, 7767, 3561, 5343, 7102, 1288, 8748, 9233, 5378, 2635, 7447, 1746, 7219, 4115, 9004, 72, 7896, 1664, 6083, 2350, 7203, 6277, 5379, 9189, 7397, 7219, 8621, 9688, 694, 9115, 876, 2174, 1715, 7810, 6357, 2581, 2313, 4227, 9883, 1643, 1421, 4565, 8296, 7576, 1811, 5831, 3384, 7444, 4699, 5968, 2909, 6753, 1331, 764, 6336, 8945, 2774, 4979, 8208, 9484, 4829, 878, 7896, 1634, 6742, 4098, 1947, 9904, 7570, 9503, 7352, 8675, 9081, 7893, 3467, 1058, 4614, 7447, 1361, 9471, 4625, 748, 1017, 7358, 6032, 8102, 3878, 2042, 4974, 1788, 3607, 5901, 3481, 2749, 8441, 455, 7471, 9706, 899, 8624, 5487, 9150, 1425, 4906, 6653, 4608, 2532, 8129, 7574, 7399, 5805, 2813, 4454, 5915, 1548, 125, 3839, 1185, 9090, 4780, 1927, 1670, 5251, 8905, 2646, 5371, 6453, 5569, 4084, 7378, 9511, 6140, 9923, 6669, 3652]

